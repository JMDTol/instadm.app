{
  "version": 2,
  "name": "instadm",
  "alias": "instadm.app",
  "builds": [
    { "src": "client/package.json", "use": "@now/static-build" },
    { "src": "api/**/*.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/api/thread/(.*)", "dest": "/api/thread" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/client/$1" }
  ]
}
